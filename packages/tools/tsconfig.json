{
  "compilerOptions": {
	"module": "commonjs",
	"importHelpers": true,
	"noImplicitAny": false,
	"outDir": "lib",
	"declaration": true,
	"pretty": true,
	"strictNullChecks": true,
	"noUnusedLocals": true,
	"target": "esnext",
	"lib": [
	  "dom",
	  "es6",
	  "scripthost",
	  "es2015.symbol",
	  "es2017.object",
	  "es2016.array.include"
	]
  },
  "include": [
	"src/**/*"
  ],
  "exclude": [
	"node_modules"
  ]
}